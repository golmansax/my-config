[core]
	editor = vim
	excludesfile = /home/holman/.gitignore_global
[pull]
	rebase = true
[push]
	default = simple
[branch]
  # Always setup 'git pull' to rebase instead of merge
  autosetuprebase = always
[color]
  diff = auto
	ui = true
[alias]
  stash-pull = !git stash && git pull && git stash pop
  stash-push = !git stash-pull || git push
